.content {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.top_box{
	position: relative;
}
.mine{
	color: #FFFFFF;
	position: absolute;
	top: 250rpx;
	font-size: 42rpx;
	left: 35rpx;
}
.logo {
	height: 424rpx;
	width: 750rpx;
}
.middle_box{
	height: 140rpx;
	width: 680rpx;
	margin-top: -80rpx;
	z-index: 99;
	border-radius: 16rpx;
	padding:20rpx 30rpx;
	box-sizing: border-box;
}
.list_title{
	width: 700rpx;
	padding-left: 20rpx;
	box-sizing: border-box;
	border-left:10rpx #007AFF solid;
	margin-top: 50rpx;
}
.list_box{
	width: 720rpx;
	height: 160rpx;
}
.list_box>view{
	color: #6F6F6F;
}
.list_box .number{
	color: #212121;
	/* justify-content: flex-end; */
	text-align: right;
	padding-right: 40rpx;
	box-sizing: border-box;
}
.list_box .number_f{
	color: #3B3B3B;
	/* justify-content: flex-end; */
	text-align: right;
	padding-right: 40rpx;
	box-sizing: border-box;
}

